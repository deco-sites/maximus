{
    "seo": {
        "type": "website",
        "title": "Storefront",
        "noIndexing": false,
        "description": "deco",
        "titleTemplate": "%s | Deco.cx"
    },
    "theme": {
        "__resolveType": "Fashion theme"
    },
    "routes": [
        [
            {
                "handler": {
                    "value": {
                        "page": {
                            "name": "Home",
                            "path": "/",
                            "sections": [
                                {
                                    "__resolveType": "Header - 01"
                                },
                                {
                                    "__resolveType": "Carousel - 01"
                                },
                                {
                                    "__resolveType": "Campaign Timer - 01"
                                },
                                {
                                    "__resolveType": "Category List - 01"
                                },
                                {
                                    "title": "Best Sellers",
                                    "products": {
                                        "ids": [],
                                        "sort": "",
                                        "count": 4,
                                        "collection": "139",
                                        "__resolveType": "apps/vtex/loaders/intelligentSearch/productList.ts"
                                    },
                                    "description": "Check these out!",
                                    "__resolveType": "site/sections/Product/ProductShelf.tsx"
                                },
                                {
                                    "__resolveType": "Shoppable Banner - 01"
                                },
                                {
                                    "__resolveType": "Testimonials - 01"
                                },
                                {
                                    "__resolveType": "Benefits - 01"
                                },
                                {
                                    "__resolveType": "Logos - 01"
                                },
                                {
                                    "__resolveType": "Newsletter - 01"
                                },
                                {
                                    "__resolveType": "FAQ - 01"
                                },
                                {
                                    "__resolveType": "Footer - 01"
                                }
                            ],
                            "__resolveType": "apps/website/pages/Page.tsx"
                        },
                        "__resolveType": "apps/website/handlers/fresh.ts"
                    }
                },
                "pathTemplate": "/"
            },
            {
                "handler": {
                    "value": {
                        "page": {
                            "name": "Product Page",
                            "path": "/:slug/p",
                            "sections": [
                                {
                                    "__resolveType": "Header - 01"
                                },
                                {
                                    "page": {
                                        "slug": {
                                            "param": "slug",
                                            "__resolveType": "apps/website/functions/requestToParam.ts"
                                        },
                                        "__resolveType": "apps/vtex/loaders/intelligentSearch/productDetailsPage.ts"
                                    },
                                    "variant": "slider",
                                    "__resolveType": "site/sections/Product/ProductDetails.tsx"
                                },
                                {
                                    "title": "You may also like",
                                    "products": {
                                        "slug": {
                                            "param": "slug",
                                            "__resolveType": "apps/website/functions/requestToParam.ts"
                                        },
                                        "count": 4,
                                        "crossSelling": "whosawalsosaw",
                                        "__resolveType": "apps/vtex/loaders/legacy/relatedProductsLoader.ts"
                                    },
                                    "__resolveType": "site/sections/Product/ProductShelf.tsx"
                                },
                                {
                                    "__resolveType": "Footer - 01"
                                }
                            ],
                            "__resolveType": "apps/website/pages/Page.tsx"
                        },
                        "__resolveType": "apps/website/handlers/fresh.ts"
                    }
                },
                "pathTemplate": "/:slug/p"
            },
            {
                "handler": {
                    "value": {
                        "page": {
                            "name": "Category",
                            "path": "/*",
                            "sections": [
                                {
                                    "__resolveType": "Header - 01"
                                },
                                {
                                    "page": {
                                        "sort": "",
                                        "count": 12,
                                        "fuzzy": "automatic",
                                        "__resolveType": "apps/vtex/loaders/intelligentSearch/productListingPage.ts",
                                        "selectedFacets": []
                                    },
                                    "__resolveType": "site/sections/Product/SearchResult.tsx"
                                },
                                {
                                    "__resolveType": "Footer - 01"
                                }
                            ],
                            "__resolveType": "apps/website/pages/Page.tsx"
                        },
                        "__resolveType": "apps/website/handlers/fresh.ts"
                    }
                },
                "pathTemplate": "/*"
            },
            {
                "handler": {
                    "value": {
                        "page": {
                            "name": "Offline",
                            "path": "/offline",
                            "sections": [
                                {
                                    "__resolveType": "Header - 01"
                                },
                                {
                                    "title": "Oh no! You are offline!",
                                    "products": {
                                        "ids": [],
                                        "sort": "price:desc",
                                        "count": 4,
                                        "collection": "139",
                                        "__resolveType": "apps/vtex/loaders/intelligentSearch/productList.ts"
                                    },
                                    "description": "Maybe take some time to explore",
                                    "__resolveType": "site/sections/Product/ProductShelf.tsx"
                                },
                                {
                                    "__resolveType": "Footer - 01"
                                }
                            ],
                            "__resolveType": "apps/website/pages/Page.tsx"
                        },
                        "__resolveType": "apps/website/handlers/fresh.ts"
                    }
                },
                "pathTemplate": "/offline"
            }
        ],
        {
            "__resolveType": "vtex/loaders/proxy.ts",
            "includeSiteMap": [],
            "extraPathsToProxy": []
        }
    ],
    "commerce": {
        "account": "bravtexfashionstore",
        "platform": "vtex",
        "publicUrl": "fashion.deco.site"
    },
    "__resolveType": "site/apps/storefront.ts"
}